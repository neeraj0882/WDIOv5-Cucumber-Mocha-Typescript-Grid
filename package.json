        "@wdio/cli": "^5.18.4",
        "@wdio/cucumber-framework": "^5.15.1",
        "@wdio/local-runner": "^5.18.4",
        "@wdio/mocha-framework": "^5.16.15",
        "@wdio/selenium-standalone-service": "^5.16.10",
        "@wdio/spec-reporter": "^5.16.11",
        "@wdio/sync": "^5.17.0",
        "allure-commandline": "^2.13.0",
        "chai": "^4.2.0",
        "cucumber-pretty": "^6.0.0",
